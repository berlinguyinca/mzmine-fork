<?xml version="1.0" encoding="utf-8"?>
<batch>
    <batchstep method="net.sf.mzmine.modules.rawdatamethods.rawdataimport.RawDataImportModule">
        <parameter name="Raw data file names">
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_R1.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_R2.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_R3.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_1to10_R1.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_1to10_R2.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_EI_Algae_SorbCon_1to10_R3.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate1_EI_1to1.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate2_EI_1to1.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate3_EI_1to1.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate1_EI_1to10.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate2_EI_1to10.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_DiabeticPlasma_Replicate3_EI_1to10.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_050913_HumanPlasma_EI.csv</file>
            <file>src/test/resources/deconvolutedanalysis/famealignment_070513_HumanPlasma_EI.csv</file>
        </parameter>
    </batchstep>

    <batchstep method="net.sf.mzmine.modules.deconvolutedanalysis.spectrafilter.SpectraFilterModule">
        <parameters>
            <parameter name="Raw data files"/>
            <parameter name="C13 Isotope Cut">0.5</parameter>
            <parameter name="Base Peak Threshold">0</parameter>
            <parameter name="Unique Mass Threshold">0</parameter>
            <parameter name="Intensity Threshold">0</parameter>
            <parameter name="Intensity Percentage Threshold">0.000</parameter>
            <parameter name="Suffix">filtered</parameter>
            <parameter name="Remove source file after filtering">true</parameter>
        </parameters>
    </batchstep>

    <batchstep method="net.sf.mzmine.modules.deconvolutedanalysis.famealignment.FameAlignmentModule">
        <parameters>
            <parameter name="Raw data files"/>
            <parameter name="EI Files">
                <item>famealignment_EI_Algae_SorbCon_R1.csv filtered</item>
                <item>famealignment_EI_Algae_SorbCon_R2.csv filtered</item>
                <item>famealignment_EI_Algae_SorbCon_R3.csv filtered</item>
                <item>famealignment_EI_Algae_SorbCon_1to10_R1.csv filtered</item>
                <item>famealignment_EI_Algae_SorbCon_1to10_R2.csv filtered</item>
                <item>famealignment_EI_Algae_SorbCon_1to10_R3.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate1_EI_1to1.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate2_EI_1to1.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate3_EI_1to1.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate1_EI_1to10.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate2_EI_1to10.csv filtered</item>
                <item>famealignment_DiabeticPlasma_Replicate3_EI_1to10.csv filtered</item>
                <item>famealignment_050913_HumanPlasma_EI.csv filtered</item>
                <item>famealignment_070513_HumanPlasma_EI.csv filtered</item>
            </parameter>
            <parameter name="PCI Files"/>
            <parameter name="Retention Time Search Window (s)">10</parameter>
            <parameter name="Suffix">ri_corrected</parameter>
            <parameter name="Remove source file after filtering">true</parameter>
            <parameter name="Show Results">false</parameter>
        </parameters>
    </batchstep>
</batch>
